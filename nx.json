{
  "npmScope": "blu",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "parallel": 1
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "fantasia",
  "generators": {
    "@schematics/angular": {
      "styleExt": "scss"
    },
    "@nrwl/schematics:library": {
      "unitTestRunner": "jest",
      "framework": "angular"
    },
    "@nrwl/schematics:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "protractor"
    },
    "@nrwl/schematics:node-application": {
      "framework": "express"
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
